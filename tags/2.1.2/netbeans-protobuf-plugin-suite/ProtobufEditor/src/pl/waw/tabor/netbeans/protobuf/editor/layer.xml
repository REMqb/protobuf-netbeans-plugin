<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Editors">
        <folder name="text">
            <folder name="x-proto">
                <file name="language.nbs" url="language.nbs"/>
                <folder name="CompletionProviders">
                    <file name="pl-waw-tabor-netbeans-protobuf-editor-ProtobufCompletionProvider.instance"/>
                </folder>
            </folder>
        </folder>
    </folder>
    <folder name="Navigator">
        <folder name="Panels">
            <folder name="text">
                <folder name="x-proto">
                    <!--file name="org-netbeans-modules-languages-features-LanguagesNavigator.instance"/-->
                </folder>
            </folder>
        </folder>
    </folder>
    <folder name="Services">
        <folder name="MIMEResolver">
            <file name="text-x-proto-mime-resolver.xml" url="text-x-proto-mime-resolver.xml"/>
        </folder>
    </folder>
    <folder name="Templates">
        <folder name="Other">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="pl.waw.tabor.netbeans.protobuf.editor.Bundle"/>
            <file name="protobuf.proto" url="protobuf.proto">
                <attr name="template" boolvalue="true"/>
                <attr name="SystemFileSystem.localizingBundle" stringvalue="pl.waw.tabor.netbeans.protobuf.editor.Bundle"/>
                <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/pl/waw/tabor/netbeans/protobuf/editor/g.gif"/>
                <attr name="templateWizardURL" urlvalue="nbresloc:/pl/waw/tabor/netbeans/protobuf/editor/protobuf.template.html"/>
                <!--Use this line only if your template makes use of the FreeMarker template language:-->
                <!--attr name="javax.script.ScriptEngine" stringvalue="freemarker"/-->
            </file>
        </folder>
    </folder>
</filesystem>
